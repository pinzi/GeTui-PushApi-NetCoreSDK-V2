<Project Sdk="Microsoft.NET.Sdk">
   <PropertyGroup> 
     <TargetFramework>net6.0</TargetFramework> 
     <ImplicitUsings>enable</ImplicitUsings> 
     <Nullable>enable</Nullable>
     <Description>个推PUSH SDK For .NetCore中使用内存存储</Description> 
     <ApplicationIcon></ApplicationIcon> 
     <PackageIcon>icon_128x128.png</PackageIcon> 
	 <iconUrl>https://s1.ax1x.com/2022/05/11/OUtE4J.jpg</iconUrl> 
     <PackageReadmeFile>README.md</PackageReadmeFile> 
     <RepositoryType>git</RepositoryType> 
     <Authors>pinzi</Authors> 
     <Product>GeTuiPushApiV2.NetCoreSDK.Core.MemoryCache</Product> 
     <GeneratePackageOnBuild>True</GeneratePackageOnBuild> 
     <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance> 
     <PackageLicenseFile>LICENSE.txt</PackageLicenseFile> 
     <PackageId>GeTuiPushApiV2.NetCoreSDK.Core.MemoryCache</PackageId> 
     <PackageTags>GeTui;GeTuiPush</PackageTags> 
     <PackageReleaseNotes>第4次发布，增加代码注释信息</PackageReleaseNotes> 
     <Copyright>Copyright 2022 (c) pinzi. All rights reserved.</Copyright> 
     <RepositoryUrl>https://github.com/pinzi/GeTui-PushApi-NetCoreSDK-V2.git</RepositoryUrl> 
     <PackageProjectUrl>https://www.nuget.org/packages/GeTuiPushApiV2.NetCoreSDK.Core</PackageProjectUrl> 
     <Version>1.0.3</Version> 
     <GenerateDocumentationFile>True</GenerateDocumentationFile> 
   </PropertyGroup> 

  <ItemGroup>
    <None Include="..\icon_128x128.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\LICENSE.txt" Link="LICENSE.txt">
      <PackagePath>\</PackagePath>
      <Pack>True</Pack>
    </None>
    <None Include="..\README.md" Link="README.md">
      <PackagePath>\</PackagePath>
      <Pack>True</Pack>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Caching.Memory" Version="6.0.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\GeTuiPushApiV2.NetCoreSDK.Core.Storage\GeTuiPushApiV2.NetCoreSDK.Core.Storage.csproj" />
  </ItemGroup>

</Project>
